{
    "name": "karu/laroute",
    "description": "Access Laravels URL/Route helper functions, from JavaScript.",
    "keywords"  : ["laravel", "routes", "routing", "javascript"],
    "authors": [
        {
            "name": "Karunaiswaran",
            "email": "karunais1392@gmail.com"
        }
    ],
    "license" : "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/support" : ">= 5.0.0",
        "illuminate/routing": ">= 5.0.0",
        "illuminate/console": ">= 5.0.0",
        "illuminate/config": ">= 5.0.0",
        "illuminate/filesystem": ">= 5.0.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : ">=4.0",
        "mockery/mockery" : "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Karu\\Laroute\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Karu\\Laroute\\LarouteServiceProvider"
            ]
        }
    }
}
